<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
        <key>Label</key>
        <string>gov.llnl.mp.py.api</string>
        <key>ServiceDescription</key>
        <string>MacPatch REST API</string>
        <key>EnvironmentVariables</key>
        <dict>
                <key>PYTHON_EGG_CACHE</key>
                <string>/opt/MacPatch/Server/apps/env</string>
        </dict>
        <key>ProgramArguments</key>
        <array>
                <string>/opt/MacPatch/Server/apps/env/bin/python</string>
                <string>/opt/MacPatch/Server/apps/mpapi.py</string>
                <string>gunicorn</string>
        </array>
        <key>KeepAlive</key>
        <true/>
        <key>RunAtLoad</key>
        <true/>
        <key>UserName</key>
        <string>_appserver</string>
        <key>StandardErrorPath</key>
        <string>/opt/MacPatch/Server/logs/mpapi_stderr.log</string>
        <key>StandardOutPath</key>
        <string>/opt/MacPatch/Server/logs/mpapi_stdout.log</string>
        <key>WorkingDirectory</key>
        <string>/opt/MacPatch/Server/apps/env/bin</string>
</dict>
</plist>